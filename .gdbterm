set confirm off

define a
    attach $arg0
end

define sb
    del
    source ./.gdbbreaks
end

define sd
    undisplay
    source ./.gdbdisplays
end

define s2
    signal 2
end

define plist
    set var $n = $arg0
    while $n
        print *($n)
        set var $n = $n->next
    end
end

define print_plugins
    set $n = state->num_plugins
    set $i = 0
    while $n > 0
        print *state->plugins[$i]
        set $i = $i + 1
        set $n = $n - 1
    end
end
    
